<settings>
  <font>Tahoma</font>
  <hide_bullets_count>18</hide_bullets_count>
  <background>
    <alpha>90</alpha>
    <border_alpha>10</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>50</left>
      <right>40</right>
      <bottom>65</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>10</alpha>
    </y_left>
  </grid>
  <legend>
    <enabled>0</enabled>
  </legend>
  <axes>
    <x>
      <width>1</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>1</width>
      <color>0D8ECF</color>
    </y_left>
  </axes>
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <context_menu>
    <default_items>
      <print>0</print>
    </default_items>
  </context_menu>
  <graphs>
    <graph gid="0">
      <title>Sumarycznie</title>
      <color>B0DE09</color>
      <line_width>3</line_width>
      <fill_alpha>30</fill_alpha>
      <bullet>round</bullet>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>Klienci</b>]]></text>
      <y>25</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>